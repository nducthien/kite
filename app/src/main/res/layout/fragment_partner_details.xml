<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/parent"
    style="@style/ActivityStyle"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:orientation="vertical" >

    <LinearLayout
        android:id="@+id/partner_ll_container"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <TextView
            android:id="@+id/partner_header"
            style="@style/BaseTextHeader"
            android:layout_width="match_parent"
            android:layout_marginTop="10dp"
            android:text="@string/dummy_text" />

        <View
            android:id="@+id/view"
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:background="@color/primary_green" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/partner_tv_partner_code"
                    style="@style/BaseTextView"
                    android:text="@string/partner_tv_partnercode"
                    android:layout_width="match_parent"
                    android:layout_weight="0.7"/>

                <EditText
                    android:id="@+id/partner_et_partnercode"
                    style="@style/BaseEditText"
                    android:maxLines="1"
                    android:layout_weight="0.3"
                    android:editable="false"
                    android:focusable="false"
                    android:clickable="false"/>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/partner_tv_name"
                    style="@style/BaseTextView"
                    android:text="@string/partner_tv_name"
                    android:layout_width="match_parent"
                    android:layout_weight="0.7"/>

                <EditText
                    android:id="@+id/partner_et_name"
                    style="@style/BaseEditText"
                    android:maxLines="1"
                    android:maxLength="60"
                    android:layout_weight="0.3"
                    android:editable="false"
                    android:focusable="false"
                    android:clickable="false"/>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/partner_tv_partner_alkozpont"
                    style="@style/BaseTextView"
                    android:text="@string/partner_tv_alkozpont"
                    android:layout_width="match_parent"
                    android:layout_weight="0.7"/>

                <EditText
                    android:id="@+id/partner_et_alkozpont"
                    style="@style/BaseEditText"
                    android:maxLines="1"
                    android:layout_weight="0.3"
                    android:editable="false"
                    android:focusable="false"
                    android:clickable="false"/>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/partner_tv_address"
                    style="@style/BaseTextView"
                    android:text="@string/partner_tv_address"
                    android:layout_width="match_parent"
                    android:layout_weight="0.7"/>

                <EditText
                    android:id="@+id/partner_et_address"
                    style="@style/BaseEditText"
                    android:maxLines="1"
                    android:layout_weight="0.3"
                    android:editable="false"
                    android:focusable="false"
                    android:clickable="false"/>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/partner_tv_email"
                    style="@style/BaseTextView"
                    android:text="@string/partner_tv_email"
                    android:layout_width="match_parent"
                    android:layout_weight="0.7"/>

                <EditText
                    android:id="@+id/partner_et_email"
                    style="@style/BaseEditText"
                    android:maxLines="2"
                    android:lines="2"
                    android:layout_weight="0.3"
                    android:editable="false"
                    android:focusable="false"
                    android:clickable="false"
                    android:maxLength="200"/>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/partner_tv_minosites"
                    style="@style/BaseTextView"
                    android:text="@string/partner_tv_minosites"
                    android:layout_width="match_parent"
                    android:layout_weight="0.7"/>

                <EditText
                    android:id="@+id/partner_et_minosites"
                    style="@style/BaseEditText"
                    android:maxLines="1"
                    android:layout_weight="0.3"
                    android:editable="false"
                    android:focusable="false"
                    android:clickable="false"/>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/partner_tv_szabad_feherlista_keret"
                    style="@style/BaseTextView"
                    android:text="@string/partner_tv_szabad_feherlista_keret"
                    android:layout_width="match_parent"
                    android:layout_weight="0.7"/>

                <EditText
                    android:id="@+id/partner_et_szabad_feherlista_keret"
                    style="@style/BaseEditText"
                    android:maxLines="1"
                    android:layout_weight="0.3"
                    android:editable="false"
                    android:focusable="false"
                    android:clickable="false"
                    android:maxLength="200" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/partner_tv_atutalasos_feherlista_keret"
                    style="@style/BaseTextView"
                    android:text="@string/partner_tv_atutalasos_feherlista_keret"
                    android:layout_width="match_parent"
                    android:layout_weight="0.7"/>

                <EditText
                    android:id="@+id/partner_et_atutalasos_feherlista_keret"
                    style="@style/BaseEditText"
                    android:maxLines="1"
                    android:layout_weight="0.3"
                    android:editable="false"
                    android:focusable="false"
                    android:clickable="false"
                    android:maxLength="200"/>
            </LinearLayout>
        </LinearLayout>

    </LinearLayout>

</LinearLayout>
